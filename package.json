    "@typescript-eslint/parser": "^6.13.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "parcel": "^2.10.3",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
